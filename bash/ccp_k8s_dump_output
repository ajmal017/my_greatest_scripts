          - /sbin/sysctl
          - -w
          - vm.max_map_count=262144
          image: registry.ci.ciscolabs.com/cpsg_ccp-charts/alpine:3.7
          imagePullPolicy: IfNotPresent
          name: elasticsearch-logging-init
