- apiVersion: apps/v1
  kind: StatefulSet
  metadata:
    creationTimestamp: 2018-08-09T15:17:15Z
    generation: 1
    labels:
      addonmanager.kubernetes.io/mode: Reconcile
      k8s-app: elasticsearch-logging
      kubernetes.io/cluster-service: "true"
    name: elasticsearch-logging
    namespace: ccp
    resourceVersion: "2016"
    selfLink: /apis/apps/v1/namespaces/ccp/statefulsets/elasticsearch-logging
    uid: 4c8dff90-9be7-11e8-ab70-005056af9d90
  spec:
    podManagementPolicy: OrderedReady
    replicas: 2
    revisionHistoryLimit: 10
    selector:
      matchLabels:
        k8s-app: elasticsearch-logging
    serviceName: elasticsearch-logging
    template:
