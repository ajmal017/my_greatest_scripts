- apiVersion: autoscaling/v1
  kind: HorizontalPodAutoscaler
  metadata:
    annotations:
      autoscaling.alpha.kubernetes.io/conditions: '[{"type":"AbleToScale","status":"True","lastTransitionTime":"2018-08-09T15:17:48Z","reason":"SucceededGetScale","message":"the
        HPA controller was able to get the target''s current scale"},{"type":"ScalingActive","status":"False","lastTransitionTime":"2018-08-09T15:17:48Z","reason":"FailedGetResourceMetric","message":"the
        HPA was unable to compute the replica count: unable to get metrics for resource
        cpu: unable to fetch metrics from resource metrics API: the server could not
        find the requested resource (get pods.metrics.k8s.io)"}]'
    creationTimestamp: 2018-08-09T15:17:18Z
    name: istio-egressgateway
    namespace: istio-system
    resourceVersion: "1922"
    selfLink: /apis/autoscaling/v1/namespaces/istio-system/horizontalpodautoscalers/istio-egressgateway
    uid: 4e089b97-9be7-11e8-ab70-005056af9d90
