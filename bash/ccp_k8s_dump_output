================
kubectl get daemonsets --all-namespaces -o yaml
================

apiVersion: v1
items:
- apiVersion: extensions/v1beta1
  kind: DaemonSet
  metadata:
    creationTimestamp: 2018-08-09T15:17:15Z
    generation: 1
    labels:
      app: metallb
      chart: metallb-0.4.0
      component: speaker
      heritage: Tiller
      release: ccp-addons
    name: ccp-addons-metallb-speaker
    namespace: ccp
    resourceVersion: "1723"
    selfLink: /apis/extensions/v1beta1/namespaces/ccp/daemonsets/ccp-addons-metallb-speaker
    uid: 4c6c44d5-9be7-11e8-ab70-005056af9d90
  spec:
    revisionHistoryLimit: 10
    selector:
      matchLabels:
        app: metallb
        component: speaker
        release: ccp-addons
    template:
      metadata:
